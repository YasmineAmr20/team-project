
       
    

  <div class="my-5 py-5 px-5 doctorInfo" id="parent"  class="addDoctorForm" style=" border: solid  rgb(53, 33, 141);">

   
    <div>
      <a href="/student/add" id="parent">
        <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" width="16" height="16" style="fill: rgb(47, 14, 235);">
          <path  d="M11 11V5H13V11H19V13H13V19H11V13H5V11H11Z"></path>
        </svg>
        Add new student
      </a>
    </div>
    
   
    <h2 class="text-center pb-5">Students Information</h2>
   
    <table class="table" id="parent" style="border: rgb(50, 7, 240);">
      <thead>
        <tr>
          <th scope="col">Student Name</th>
          <th scope="col">Academic Number</th>
          <th scope="col">Student Password</th>
          <th scope="col"></th>
          <th scope="col"></th>
        </tr>
      </thead>
      <tbody>
        {{#each student}}
        <tr>
          <td>{{std_name}}</td>
          <td>{{std_Academic}}</td>
          <td>{{std_password}}</td>
          <td></td>
          <td>

            <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" width="24" height="24" style="fill:rgb(38, 8, 168);">
              <path
                d="M15.7279 9.57629L14.3137 8.16207L5 17.4758V18.89H6.41421L15.7279 9.57629ZM17.1421 8.16207L18.5563 6.74786L17.1421 5.33365L15.7279 6.74786L17.1421 8.16207ZM7.24264 20.89H3V16.6474L16.435 3.21233C16.8256 2.8218 17.4587 2.8218 17.8492 3.21233L20.6777 6.04075C21.0682 6.43128 21.0682 7.06444 20.6777 7.45497L7.24264 20.89Z"
                ></path>
            </svg>
            <a href="/student/{{_id}}/edit" class="editDoctorLink">
              Edit 
            </a>
            |
              <form action="/student/{{_id}}?_method=DELETE" method="POST"  style="display: inline;">

                <button  type="submit" class="btn btn-link delete"   onclick="return confirm('Are you sure you wish to delete this Subject?');">
                  <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" width="24" height="24" style="fill: rgb(248, 17, 17);">
                    <path
                      d="M7 4V2H17V4H22V6H20V21C20 21.5523 19.5523 22 19 22H5C4.44772 22 4 21.5523 4 21V6H2V4H7ZM6 6V20H18V6H6ZM9 9H11V17H9V9ZM13 9H15V17H13V9Z"
                     >
                    </path>
                  </svg>
                  

                  Delete
                </button>
              </form>
          </td>
        </tr>
        {{/each}}
      </tbody>
    </table>
  </div>
  